    "@storybook/web-components-vite": "latest",
    "@whitespace/storybook-addon-html": "latest",
    "gh-pages": "latest",
    "react": "latest",
    "react-dom": "latest",
    "storybook": "latest",
    "typescript": "latest",
    "vite": "latest"
  }
